#include <iostream>
using namespace std;
int main ()
{
    int a,b,c;
    cout<<"enter a:";
    cin>>a;
    cout<<"enter b:";
    cin>>b;
    cout<<"enter c:";
    cin>>c;
    if((a>=b)&&(a>=c))
    cout<<"a is biggest";
    else if ((b>=c)&&(b>=a))
    cout<<"b is biggest";
    else
    cout<<"c is biggest";
    return 0;
}
